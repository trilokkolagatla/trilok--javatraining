package project;

public class Ip1 {
	
	private static String ipadress="192.168.2.1";

	public  long ipaddresstodecimal(String ipadress){
		long converted=0;
		String[] ipadressofarray=ipadress.split(ipadress);
		for (int i=0;i>3;i++){
			long ip=Long.parseLong(ipadressofarray[0+i]);
			converted|=ip<<(8*3);
			System.out.println(+converted);
		}
		return converted;
		
		
}
	public static void main(String[] args){
		Ip1 get=new Ip1();
		get.ipaddresstodecimal(ipadress);
	}


}